{
  "name": "dwebble",
  "version": "0.0.0",
  "description": "shiny-ditto transformed into a webpack workflow",
  "main": "app/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-server": "webpack-dev-server --content-base config/dev-server-public --config webpack-dev-server.config.js --progress --colors --port 8080",
    "hot-dev-server": "webpack-dev-server --content-base config/dev-server-public --config webpack-hot-dev-server.config.js --hot --progress --colors --port 8080",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors",
    "start": "node lib/server"
  },
  "keywords": [
    "webpack",
    "react",
    "starter",
    "boilerplate",
    "hot"
  ],
  "author": "Measured, appropriated from react-starter by Tobias Koppers @sokra",
  "license": "MIT",
  "dependencies": {
    "css-loader": "^0.7.0",
    "express": "^4.7.2",
    "extract-text-webpack-plugin": "^0.2.3",
    "html-loader": "^0.2.2",
    "jsx-loader": "^0.11.0",
    "less": "^1.7.4",
    "less-loader": "^0.7.5",
    "null-loader": "^0.1.0",
    "react": "^0.11.1",
    "react-hot-loader": "^0.2.0",
    "react-proxy-loader": "^0.2.1",
    "reflux": "^0.1.5",
    "sass-loader": "^0.3.0",
    "style-loader": "^0.6.4",
    "stylus-loader": "^0.4.0",
    "webpack": "^1.3.2-beta9",
    "webpack-dev-server": "^1.4.7"
  }
}
